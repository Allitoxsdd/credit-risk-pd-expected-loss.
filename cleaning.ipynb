    "# ------------------------------------------------------------------------------\n",
    "# 7. QUICK TARGET vs FEATURE CHECKS (high-level, still Stage 1-ish)\n",
    "# ------------------------------------------------------------------------------\n",
    "\n",
    "def default_rate_by_category(df: pd.DataFrame, col: str, target: str = TARGET_COL):\n",
    "    \"\"\"\n",
    "    Compute default rate for each category of a given categorical feature.\n",
    "    \"\"\"\n",
    "    out = (\n",
    "        df.groupby(col)[target]\n",
    "        .mean()\n",
    "        .rename(\"default_rate\")\n",
    "        .to_frame()\n",
    "        .sort_index()\n",
    "    )\n",
    "    return out\n",
    "\n",
    "\n",
    "#(most recent payment status)\n",
    "dr_pay0 = default_rate_by_category(df, \"PAY_0\")\n",
    "print(\"\\nDefault rate by PAY_0:\")\n",
    "print(dr_pay0)\n",
    "\n",
    "\n",
    "df[\"LIMIT_BAL_BIN\"] = pd.qcut(df[\"LIMIT_BAL\"], q=5, duplicates=\"drop\")\n",
    "dr_limit = (\n",
    "    df.groupby(\"LIMIT_BAL_BIN\")[TARGET_COL]\n",
    "    .mean()\n",
    "    .rename(\"default_rate\")\n",
    "    .to_frame()\n",
    ")\n",
    "print(\"\\nDefault rate by LIMIT_BAL quintiles:\")\n",
    "print(dr_limit)\n",
    "\n",
    "\n",
    "df = df.drop(columns=[\"LIMIT_BAL_BIN\"])"
